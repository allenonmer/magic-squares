{
  "version": "0.1.5",
  "manifest_version": 3,
  "name": "Magic Squares",
  "offline_enabled": true,
  "permissions": ["storage"],
  "description": "An easy-to-use and lite Magic Squares game",
  "homepage_url": "https://mybrowseraddon.com/magic-squares.html",
  "commands": {
    "_execute_action": {}
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Magic Squares",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
